
NAME MANGLING

extern "C" {
  void first_func();
  int second_fund(int a, double b);
}

extern "C" void abc();

extern "C" - tells compiler not to mangle the name of a function

#ifdef __cplusplus
 extern "C" {
#endif
  void a(int x1, int y1, int x2, int y2);
  void b(unsigned char bits);
  void c(int iterations);
#ifdef __cplusplus
 }
#endif
